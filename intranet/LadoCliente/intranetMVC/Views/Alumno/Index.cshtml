
@model IEnumerable<intranetMVC.WCFCliente.Alumno>

@{
    ViewBag.Title = "Mantenimiento de Alumnos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="js/jquery.min.js" type="text/javascript"></script>
<script src="js/jquery.dataTables.min.js" type="text/javascript"></script>
<p>
    @*@Html.ActionLink("Create New", "Create")*@
    <form>
        <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
            <div class="col-md-4">
                <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
            </div>
        </div>
        <div class="form-group row">
            <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>
            <div class="col-md-4">
                <input type="password" class="form-control" id="inputPassword3" placeholder="Password">
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-2">Checkbox</div>
            <div class="col-sm-10">
                <div class="form-check">
                    <label class="form-check-label">
                        <input class="form-check-input" type="checkbox"> Check me out
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <button type="submit" class="btn btn-primary" onclick="location.href='@Url.Action("Create", "Alumno")';return false;">Registrar</button>
                <button type="submit" class="btn btn-danger">Eliminar</button>
            </div>
        </div>
    </form>
</p>

<div id="myTable"></div>
<table class="table">
    <tr>
        <td>
            <input type="checkbox" name="name5" />
        </td>
        <th>
            @Html.DisplayNameFor(model => model.IdAlumno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApePatAlumno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApeMatAlumno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomAlumno)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.CodigoAlu)
            </th>*@
        <th>
            @Html.DisplayNameFor(model => model.DirAlumno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailAlumno)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Escuela)
            </th>*@
        <th>
            @Html.DisplayNameFor(model => model.TelAlumno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DNI)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                <input type="checkbox" name="name5" />
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.IdAlumno)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.ApePatAlumno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApeMatAlumno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomAlumno)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.CodigoAlu)
                </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.DirAlumno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailAlumno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelAlumno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DNI)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdAlumno }) |
                @Html.ActionLink("Search", "Search", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }
</table>


